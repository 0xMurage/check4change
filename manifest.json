{
  "author": "murage@labskenya.com",
  "name": "Another check for change",
  "version": "0.0.1",
  "description": "Another check 4 change to monitor changes automatically",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "alarms",
    "notifications",
    "https://*/",
    "http://*/"
  ],
  "background": {
    "scripts": [
      "assets/scripts/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "pages/popup.html",
    "default_icon": {
      "16": "assets/images/sample_16.png",
      "128": "assets/images/128x128.png"
    }
  },
  "web_accessible_resources": [
    "pages/selection-frame.html",
    "assets/styles/iframe.css",
    "assets/styles/iframe.js",
    "assets/styles/selector.css"
  ],
  "icons": {
    "16": "assets/images/sample_16.png",
    "128": "assets/images/128x128.png"
  }
}
